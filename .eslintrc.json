{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard-with-typescript", "eslint-config-prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"],
    "extraFileExtensions": [".json"]
  },
  "rules": {
    "no-unused-expressions": "off"
  }
}
